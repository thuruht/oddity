// wrangler.jsonc
{
  "name": "odd-mapchan",
  "main": "src/index.ts",
  "compatibility_date": "2024-06-14",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "assets": {
    "binding": "ASSETS",
    "directory": "./public"
  },
  "vars": {
    "R2_PUBLIC_URL": "https://odd-img.distorted.work"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "odd-db",
      "database_id": "01f55f50-455e-49e6-81de-9724351abda5"
    }
  ],
  "r2_buckets": [
    {
      "binding": "R2_BUCKET",
      "bucket_name": "odd-media"
    }
  ]
}